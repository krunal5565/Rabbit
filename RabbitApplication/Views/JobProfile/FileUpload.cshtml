
@{
    ViewData["Title"] = "FileUpload";
    Layout = "~/Views/Shared/_CandidateLayout.cshtml";
}

<h1>Upload Files</h1>

<form method="post" enctype="multipart/form-data" asp-controller="JobProfile" asp-action="FileUpload">
     <div class="form-group">
        <div class="col-md-10">
            <input type="text" name="fileType"  />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="file" name="files"  />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>