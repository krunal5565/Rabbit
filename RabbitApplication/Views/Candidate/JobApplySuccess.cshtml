@model RabbitApplication.Models.JobApplySuccessModel

@{
	ViewData["Title"] = "Job Apply Success";
	Layout = "~/Views/Shared/_GuestLayout.cshtml";
}

<main id="main">
	<br /><br />
	<section id="values" class="values">
		<div class="container" data-aos="fade-up">

			<div class="row">
				<div class="col-lg-12" data-aos="fade-up" data-aos-delay="200">
					<div class="box">
						<header class="section-header">
							<p>Your Job application has been successfully submited.</p>
							<br />
							<h3>	Your acknowledgment number is <b> <u>@Model.AkNo</u>.</b></h3>
						</header>
					</div>
				</div>
			</div>
		</div>
	</section>
</main>



