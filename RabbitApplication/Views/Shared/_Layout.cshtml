            background-color: #3276b1;
            color: white;
            padding: 5px;
        }

        .bg-navbar {
            background-color: #2e4b90 !important;
        }

        .sidebar-light .sidebar-brand {
            color: #fafafa;
            background-color: #2e4b90 !important;
            border-right: 1px solid white;
        }

        .div-header {
            background-color: #f5f5f5;
            border: #ddd;
            border: 1px solid #ddd;
            color: black;
            padding: 5px;
            border-radius: 10px 10px 0px 0px;
        }
    </style>
